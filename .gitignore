# Ignore build output directories
/target/
/build/

# Ignore IDE and tool-specific directories and files
/src/.idea/
/.project
/.classpath
/.settings/
/.vscode/
/src/*.iml
/src/.gitignore

# Ignore Maven and Gradle wrapper files
/.mvn/wrapper/maven-wrapper.jar
/gradle/wrapper/gradle-wrapper.jar

# Ignore logs, temp files, and other unimportant files
*.log
*.tmp
*.bak
*.swp

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore compiled class files
*.class

# Ignore JAR files
*.jar

# Ignore environment and sensitive files
.env
secrets.txt

# Ignore Java lint and coverage tools
/.checkstyle/
/.coverage/
